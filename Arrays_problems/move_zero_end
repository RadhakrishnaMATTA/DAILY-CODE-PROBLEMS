
		
		int j=0;
		for(int i=0;i<arr.length;i++){
		    
		    
		    if(arr[i]!=0){
		        // swap logic
		        int temp = arr[i];// temp =4 
		        arr[i]=arr[j] ;// arr[0]= 4 
		        arr[j]= temp;
		   
		        
		   
		   
		   
		   
		        
		        j++;
		        
		    }
		    
		    
		    
		}
		
		for( int ohj : arr){
		    System.out.println(ohj);
		}
		
