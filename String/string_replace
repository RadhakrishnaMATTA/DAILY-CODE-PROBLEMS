
public class Main
{
	public static void main(String[] args) {
		//System.out.println("Hello World");
		
		
		String s ="zradhakrishna";
		char org='m';
		char rep='z';
		
		char arr[] =s.toCharArray();
		
		for(int i=0;i<arr.length;i++){
		    
		    
		    if(arr[i]==rep){
		        
		        
		        arr[i]=org;
		    }
		}
		
		String newone = new String(arr);
		System.out.println(newone);
		
		
		
	}
}
