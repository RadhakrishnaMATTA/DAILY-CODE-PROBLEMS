   

public class Main
{
	public static void main(String[] args)
	
	{
	//	System.out.println("Hello World");
		
		
		int arr[] ={1,2,3,4,5};
		
		String s ="Radha";
		
		
		
		for(int i=0;i<s.length();i++)
		{
		    
		    
		           for(int j=i;j<s.length();j++)
		           {
		        
		       
		       
		                      for(int k=i;k<=j;k++)
		                      {
		           
		                        // System.out.print(arr[k] +" ");
		                        
		                        System.out.print(s.charAt(k));
		                          
		                          
		                      }
		                      System.out.println( );
		       
		       
		                       }
		                       
	               }
		
		
		
	
		
	  }
}
